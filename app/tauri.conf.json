{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "App",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "毕业设计 - Hughsean Feng",
        "width": 750,
        "height": 515,
        "resizable": false,
        "center": true,
        "visible": false
      }
    ],
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "cn.fxs.hfut",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "shortDescription": "QUIC-based audio and video communication client.",
      "longDescription": "QUIC-based audio and video communication client.",
      "copyright": "© 2024 凤旭昇. All Rights Reserved."
    }
  }
}